{
  "name": "common-poc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-native": "0.61.2",
    "webpack": "4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "3.7.1"
  },
  "scripts": {
    "preinstall": "cd ios && pod install",
    "start:web": "webpack-dev-server --config webpack.config.web.js",
    "build:web": "webpack --config webpack.config.web.js",
    "start:native": "react-native run-ios",
    "lint": "eslint './**/*.{js,jsx}'",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "eslintIgnore": [
    "coverage",
    "native",
    "webpack.config.web.js"
  ],
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-tests.js"
    ],
    "transform": {
      "^.*native.*.js(x)?$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.*web.*.js(x)?$": "<rootDir>/test-transformer.web.js"
    }
  }
}
